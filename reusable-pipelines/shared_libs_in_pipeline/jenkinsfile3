library identifier: 'Jenkins_Pipeline_Shared_Libs@main',
        retriever: modernSCM([$class: 'GitSCMSource', remote: 'https://github.com/kumar2313/Jenkins_Pipeline_Shared_Libs.git'])

def config = [name: 'Newman', dayOfWeek: 'Friday']

def tags = [
    "zookeeper",
    "postgres",
    "postgres-maps",
    "kudu-master",
    "redis-master",
    "consul",
    "dcos-bootstrap"]

pipeline {
    agent any
    stages {
        stage('Audit tools') {
            steps {
                //auditTools3(config)
                echo "Audit tools"
                run_linux("${tags}")
                echo "Audit tools"
            }
        }
    }
}


def run_linux(list) {
    list.each { item ->
        echo "Hello ${item}"
    }
}